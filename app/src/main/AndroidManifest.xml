<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zeynep.ybu_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="tr.edu.ybu.eventandroid.open_welcome"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="tr.edu.ybu.eventandroid.DuyuruActivity"
            android:label="@string/title_activity_duyuru"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="tr.edu.ybu.eventandroid.KulupListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="tr.edu.ybu.eventandroid.SignupActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="tr.edu.ybu.eventandroid.SigninActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="tr.edu.ybu.eventandroid.SelectionActivity"
            android:screenOrientation="portrait" />
        <activity android:name="tr.edu.ybu.eventandroid.ClubDetail" />
        <activity android:name="tr.edu.ybu.eventandroid.EventDetailActivity"></activity>
    </application>

</manifest>